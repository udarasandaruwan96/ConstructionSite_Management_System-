<%- include('layout/header')%>
<div>
  <div>
    <div>
      <% if (message) { %>
      <div
        class="alert alert-dismissible fade show alert-<%= message.type %>"
        role="alert"
      >
        <button
          class="btn-close"
          type="button"
          data-bs-dismiss="alert"
          aria-label="Close"
        ></button>
        <strong> <%= message.message %> </strong>
      </div>
      <% } %>
      <!-- Team Start -->
      <div class="team">
        <form
          class="form-inline my-2 my-lg-0"
          action="/searchClientIntiriorDesigner"
          method="get"
        >
          <input
            class="form-control mr-sm-1"
            style="margin-left: 1000px; margin-top: -22pt"
            type="text"
            name="IntiriorDesignersearch"
            placeholder="Search"
          />
          <button
            class="btn btn-secondary my-9 my-sm-10"
            style="margin-top: -22pt"
            type="submit"
          >
            Search
          </button>
        </form>
        <div class="container">
          <div class="section-header text-center">
            <p>Our Team</p>
            <h2>Meet Our Intirior Designer</h2>
          </div>
          <div class="row">
            <%if(intiriorDesigner!=''){%> <%
            intiriorDesigner.forEach((row,index)=>{%>
            <div class="col-lg-3 col-md-6 wow fadeInUp" data-wow-delay="0.1s">
              <div class="team-item">
                <div class="team-img">
                  <a href="/clientIntiriorDesignerProject/<%=row._id%>"
                    ><img
                      src="<%=row.ProfilePicture%>"
                      alt="Team Image"
                      width="40px"
                      height="300px"
                  /></a>
                </div>
                <div class="team-text">
                  <h2><%= row.Name%></h2>
                  <br />
                  <p><%= row.Qualifications%></p>

                  <p></p>
                  <p><%= row.Phone%></p>

                  <p></p>
                  <p><%= row.Email%></p>

                  <p></p>
                </div>

                <div class="team-social">
                  <a class="social-tw" href=""
                    ><i class="fab fa-twitter"></i
                  ></a>
                  <a class="social-fb" href=""
                    ><i class="fab fa-facebook-f"></i
                  ></a>
                  <a class="social-li" href=""
                    ><i class="fab fa-linkedin-in"></i
                  ></a>
                  <a class="social-in" href=""
                    ><i class="fab fa-instagram"></i
                  ></a>
                </div>
              </div>
            </div>
            <%})%> <%}else{%>
            <h1 class="text-center text-secondary mt-5">
              No Intirior Designer found in the database !
            </h1>
            <%}%>
          </div>
        </div>
      </div>
      <!-- Team End -->
    </div>
  </div>
</div>

<%- include('layout/footer')%>
