            <div class = "frameClientGDForm">
                <div class="container">
                    <div>
                        <div>
                            <h3 class="container mt-5 d-flex justify-content-center">Garden Designer Requirements</h3>
            
                            <% if(typeof alert != 'undefined') { %>
                                <% alert.forEach(function(error) { %>
                                    <div class="alert alert-danger alert-dismissible fade show" role="alert">
                                        <strong><%= error.msg %></strong>
                                        <button class="btn-close" type="button" data-bs-dismiss="alert" aria-label="Close"></button>
                                          <span aria-hidden="true">&times;</span>
                                        </button>
                                      </div>
                                <% }) %>
                            <% } %>
                            <form action="/addClientGDReq/<%= adminGds._id %>" method="post" id="Ã¤dd-form">
                               
                                <div class="form-group row mt-3">
                                    <label for="GDName" class="col-sm-2 col-form-label" >Garden Designer</label>
                                    <div class="col-sm-8">
                                       <input type="text" class="form-control disableFeild" name="GDName" value="<%= adminGds.Name%>"></center>
                                    </div>
                                </div>

                                <div class="form-group row mt-3">
                                    <label for="ClientName" class="col-sm-2 col-form-label">Client Name</label>
                                    <div class="col-sm-8">
                                        <input type="text" class="form-control" name="ClientName" placeholder="Client Name" required>
                                    </div>
                                </div>
            
                                <div class="form-group row mt-3">
                                    <label for="Email" class="col-sm-2 col-form-label">Email</label>
                                    <div class="col-sm-8">
                                        <input type="text" class="form-control" name="Email" placeholder="Email" required>
                                    </div>
                                </div>
            
                                <div class="form-group row mt-3">
                                    <label for="Phone" class="col-sm-2 col-form-label">Mobile Number</label>
                                    <div class="col-sm-8">
                                        <input type="number" class="form-control" name="Phone" placeholder="mobile" required>
                                    </div>
                                </div>

                                <div class="form-group row mt-3">
                                    <label for="GardenArea" class="col-sm-2 col-form-label">Garden Area</label>
                                    <div class="col-sm-8">
                                        <input type="number" class="form-control" name="GardenArea" placeholder="Garden Area" required>
                                    </div>
                                </div>
    
                                <div class="form-group row mt-3">
                                    <label for="Budget" class="col-sm-2 col-form-label">Estimated Budget</label>
                                    <div class="col-sm-8">
                                        <input type="number" class="form-control" name="Budget" placeholder="Estimated Budget" required>
                                    </div>
                                </div>
            
            
                                <div class="form-group row mt-3">
                                    <label for="Address" class="col-sm-2 col-form-label">Address</label>
                                    <div class="col-sm-8">
                                        <input type="text" class="form-control" name="Address" placeholder="Address" required>
                                    </div>
                                </div>
            
                                <div class="form-group row mt-3">
                                    <label for="Grass" class="col-sm-2 col-form-label">Grass</label>
                                    <div class="col-sm-8">
                                        <select 
                                        class="form-control"  
                                        name="Grass"
                                        required
                                        >
                                        <option value="" disabled selected hidden>Please Choose...</option>
                                        <option value = "Required">Required</option>
                                        <option value = "Not Required">Not Required</option>
                                        </select>
                                        <!-- <div id="grass_type" style="display: none;">
                                            <label>Grass Type:</label>
                                            <input type="text" name="grassType" id="grassType">
                                        </div> -->
                                    </div>
                                </div>
            
                                <div class="form-group row mt-3">
                                    <label for="Pool" class="col-sm-2 col-form-label">Swimming Pool</label>
                                    <div class="col-sm-8">
                                        <select 
                                        class="form-control"  
                                        name="Pool"
                                        required
                                        >
                                        <option value="" disabled selected hidden>Please Choose...</option>
                                        <option value = "Required">Required</option>
                                        <option value = "Not Required">Not Required</option>
                                        </select>
                                    </div>
                                </div>
            
                                <div class="form-group row mt-3">
                                    <label for="Interlock" class="col-sm-2 col-form-label">Interlock Garden</label>
                                    <div class="col-sm-8">
                                        <select 
                                        class="form-control"  
                                        name="Interlock"
                                        required
                                        >
                                        <option value="" disabled selected hidden>Please Choose...</option>
                                        <option value = "Required">Required</option>
                                        <option value = "Not Required">Not Required</option>
                                        </select>
                                    </div>
                                </div>

                                <div class="form-group row mt-3">
                                    <label for="Play" class="col-sm-2 col-form-label">Kids Play area </label>
                                    <div class="col-sm-8">
                                        <select 
                                        class="form-control"  
                                        name="Play"
                                        required
                                        >
                                        <option value="" disabled selected hidden>Please Choose...</option>
                                        <option value = "Required">Required</option>
                                        <option value = "Not Required">Not Required</option>
                                        </select>
                                    </div>
                                </div>

                                <div class="form-group row mt-3">
                                    <label for="Flower" class="col-sm-2 col-form-label">Flower Plants</label>
                                    <div class="col-sm-8">
                                        <select 
                                        class="form-control"  
                                        name="Flower"
                                        required
                                        >
                                        <option value="" disabled selected hidden>Please Choose...</option>
                                        <option value = "Required">Required</option>
                                        <option value = "Not Required">Not Required</option>
                                        </select>
                                    </div>
                                </div>

                                <div class="form-group row mt-3">
                                    <label for="Pond" class="col-sm-2 col-form-label">Fish Pond</label>
                                    <div class="col-sm-8">
                                        <select 
                                        class="form-control"  
                                        name="Pond"
                                        required
                                        >
                                        <option value="" disabled selected hidden>Please Choose...</option>
                                        <option value = "Required">Required</option>
                                        <option value = "Not Required">Not Required</option>
                                        </select>
                                    </div>
                                </div>
            
                                <div class="form-group row mt-3">
                                    <label for="SpecialReq" class="col-sm-2 col-form-label">Special Requrements</label>
                                    <div class="col-sm-8">
                                        <textarea class="form-control" class="form-control" name="SpecialReq"
                                            placeholder="Special Requirements" rows="4" maxlength="200"></textarea>
                                    </div>
                                </div>
                                <center>
                                <button type="submit" class="btn btn-success col-sm-1 mt-3"><b>Submit</b></button>
                                </center>
                            </form>
                        <center>
                            <a href='/clientGDProj/<%= adminGds._id %>'><button class='btn btn-info col-sm-1 mt-3'><b>Clear</b></button></a>
                        </center>
                        </div>
                    </div>
                </div>
            </div>
            



<%- include('layout/footer')%> 