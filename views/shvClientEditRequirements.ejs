                                <% alert.forEach(function(error) { %>
                                    <div class="alert alert-danger alert-dismissible fade show" role="alert">
                                        <strong><%= error.msg %></strong>
                                        <button class="btn-close" type="button" data-bs-dismiss="alert" aria-label="Close"></button>
                                          <span aria-hidden="true">&times;</span>
                                        </button>
                                      </div>
                                <% }) %>
                            <% } %>
                            
                            <form action="/updateClientGD/<%= clientGD._id %>" method="post" id="Ã¤dd-form">
                               
                                <div class="form-group row mt-3">
                                    <label for="GDName" class="col-sm-2 col-form-label" >Garden Designer</label>
                                    <div class="col-sm-8">
                                       <input type="text" class="form-control disableFeild" name="GDName" value="<%= clientGD.GDName%>"></center>
                                    </div>
                                </div>

                                <div class="form-group row mt-3">
                                    <label for="ClientName" class="col-sm-2 col-form-label">Client Name</label>
                                    <div class="col-sm-8">
                                        <input type="text" class="form-control" name="ClientName" placeholder="Client Name" value="<%= clientGD.ClientName %>" required>
                                    </div>
                                </div>
            
