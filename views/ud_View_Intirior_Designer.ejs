<%- include('layout/header')%>
    <div>
        <div>
            <div>
                <% if (message) { %>
                    <div class="alert alert-dismissible fade show alert-<%= message.type %>" role="alert">
                        <button class="btn-close" type="button" data-bs-dismiss="alert" aria-label="Close"></button>
                        <strong>
                            <%= message.message %>
                        </strong>
                    </div>
                    <% } %>
                        <!-- Team Start -->
                        <div class="team">
                            <form class="form-inline my-2 my-lg-0" action="/searchIntiriorDesigner" method="get">
                                <input class="form-control mr-sm-1" style="margin-left: 1000px; margin-top: -22pt;" type="text" name="IntiriorDesignersearch" placeholder="Search">
                                <button class="btn btn-secondary my-9 my-sm-10" style="margin-top: -22pt;" type="submit">Search</button>
                            </form>
                            <div class="container">
                                <div class="section-header text-center">
                                    <p>Our Team</p>
                                    <h2>Meet Our Interior Designers</h2>
                                </div>
                                <div class="row">
                                    <%if(intiriorDesigner!=''){%>
                                        <% intiriorDesigner.forEach((row,index)=>{%>
                                            <div class="col-lg-3 col-md-6 wow fadeInUp" data-wow-delay="0.1s">
                                                <div class="team-item">
                                                    <div class="team-img">
                                                        <a href="/allIntiriorDesignerProject/<%=row._id%>"><img
                                                                src="<%=row.ProfilePicture%>" alt="Team Image" width="40px"
                                                                height="300px"></a>
                                                    </div>
                                                    <div class="team-text">
                                                        <h2>
                                                            <%= row.Name%>
                                                        </h2>
                                                        <p>
                                                            <%= row.Phone%>
                                                                <p>
                                                                    <a href="/editIntiriorDesigner/<%=row._id%>"
                                                                        class="text-success"> <button
                                                                            class="btn btn-outline-warning"><i
                                                                                class="fa-solid fa-pen-to-square"></i>
                                                                            Edit</button></a>
                                                                    <a href="/deleteIntiriorDesigner/<%=row._id%>"
                                                                        class="text-success"><button
                                                                            class="btn btn-outline-danger" onclick="return confirm('Are you sure you want to Delete Intirior Designer?');"><i
                                                                                class="fa-solid fa-trash"></i>Delete</button>
                                                                    </a>
                                                    </div>
                                                    <div class="team-social">
                                                        <a class="social-tw" href=""><i class="fab fa-twitter"></i></a>
                                                        <a class="social-fb" href=""><i
                                                                class="fab fa-facebook-f"></i></a>
                                                        <a class="social-li" href=""><i
                                                                class="fab fa-linkedin-in"></i></a>
                                                        <a class="social-in" href=""><i
                                                                class="fab fa-instagram"></i></a>
                                                    </div>
                                                </div>
                                            </div>
                                            <%})%>
                                                <%}else{%>
                                                    <h1 class="text-center text-secondary mt-5">No Interior Designers found in
                                                        the database!</h1>
                                                    <%}%>
                                </div>
                            </div>
                        </div>
                        <!-- Team End -->
                        <center><a href="/addIntiriorDesigner"><button type="button" class="btn btn-success">ADD INTERIOR DESIGNER
                        </button></a>
                        </center>
            </div>
        </div>
    </div>

    <%- include('layout/footer')%>